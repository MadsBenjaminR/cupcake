<+>/* Stylesheet for the application */\n\nh1 {\n    color: #369;\n    font-family: Arial, Helvetica, sans-serif;\n    font-size: 3rem;\n}\n\nbody, html {\n    height: 100%;\n    margin: 0;\n    background-color: #54037A;\n}\n\n.container {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    height: 100vh;\n}\n\n.centered-dropdowns {\n    text-align: center;\n}\n\n.centered-dropdowns select {\n    width: 300px;\n    padding: 10px;\n    margin: 5px auto;\n    border: 1px solid #ccc;\n    border-radius: 4px;\n    box-sizing: border-box;\n}\n\n/* Increase button size */\n.submit-container input[type=\"submit\"] {\n    padding: 15px 30px; /* Adjust padding as needed */\n    font-size: 18px; /* Adjust font size as needed */\n}\n\n.dropdown-container {\n    display: flex;\n    align-items: center;\n    justify-content: center; /* Horizontally center the dropdowns */\n}\n\n.VÃ¦lg-antal select {\n    margin-left: 50px; /* Adjust as needed */\n}\n\n.submit-container {\n    margin-top: 50px; /* Adjust as needed */\n}\n\n.orange-border {\n    border-top: 30px solid #F06C41;\n    width: 100%;\n}\n\n.light-orange-border {\n    border-top: 30px solid #E68C52;\n    width: 100%;\n}\n\n.pink-border {\n    border-top: 30px solid #CA4267;\n    width: 100%;\n}\n\n.edit-item-form {\n    display: flex;\n    flex-direction: column;\n    justify-content: flex-start;\n    width: 400px;\n    gap: 10px;\n}\n\n.edit-item-form input {\n    padding: 5px;\n    border-radius: 5px;\n}\n\n.btn {\n    padding: 10px;\n    border-radius: 5px;\n    background-color: #CA4267;\n    color: white;\n    font-size: 1.2rem;\n    cursor: pointer;\n    margin-top: 20px;\n}\n\n/* BOXES*/\n\n.large-box {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n}\n\n\n.add-item {\n    display: flex;\n    flex-direction: row;\n    justify-content: space-around;\n}\n\n.add-item-1 {\n    display: flex;\n    flex-direction: column;\n}\n\n.add-item-2 {\n    display: flex;\n    flex-direction: column;\n}\n\n\n.scrollable-table {\n    width: 100%;\n    height: 200px;\n    overflow-y: auto;\n    border: 1px solid #F06C41;\n}\n\ntable {\n    width: 100%;\n    border-collapse: collapse;\n}\n\nth, td {\n    text-align: left;\n    padding: 8px;\n    border: 1px solid #CA4267;\n}\n\nth {\n    background-color: #F06C41;\n}\n\n#table-styling {\n    font-family: Arial, Helvetica, sans-serif;\n    border-collapse: collapse;\n    width: 100%;\n}\n\n#table-styling td, #table-styling th {\n    border: 1px solid #CA4267;\n    padding: 8px;\n}\n\n#table-styling tr:nth-child(even) {\n    background-color: #f2f2f2;\n}\n\n#table-styling tr:hover {\n    background-color: #E68C52\n}\n\n#table-styling th {\n    padding-top: 12px;\n    padding-bottom: 12px;\n    text-align: left;\n    background-color: #F06C41;\n    color: white;\n}\n\n.buy-login {\n    position: absolute;\n    left: 90%;\n    color: white;\n}\n.sum{\nposition: relative;\n    left: 90%;\n    color: white;\n}\n
